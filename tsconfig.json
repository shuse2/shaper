{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "target": "es2019",
        "module": "commonjs",
        "moduleResolution": "node",
        "newLine": "lf",
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "pretty": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "strict": true,
        "declaration": true,
		"outDir": "lib",
    }
}